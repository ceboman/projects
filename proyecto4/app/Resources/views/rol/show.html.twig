{% extends 'base.html.twig' %}

{% block body %}
    <h1>Rol</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Idrol</th>
                <td>{{ rol.idRol }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ rol.descripcion }}</td>
            </tr>
            <tr>
                <th>Usuariocreacion</th>
                <td>{{ rol.usuarioCreacion }}</td>
            </tr>
            <tr>
                <th>Fechacreacion</th>
                <td>{% if rol.fechaCreacion %}{{ rol.fechaCreacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Usuarioactualizacion</th>
                <td>{{ rol.usuarioActualizacion }}</td>
            </tr>
            <tr>
                <th>Fechaactualizacion</th>
                <td>{% if rol.fechaActualizacion %}{{ rol.fechaActualizacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('rol_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('rol_edit', { 'idRol': rol.idRol }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
